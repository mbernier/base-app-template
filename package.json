{
  "name": "base-app-template",
  "version": "1.0.0",
  "private": true,
  "description": "Base Mini App template - Foundation for building Base Mini Apps with SIWE, OnchainKit, and Supabase",
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "next start -p 3100",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "cp node_modules/@coinbase/onchainkit/dist/assets/style.css public/onchainkit.css 2>/dev/null || true"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^1.1.2",

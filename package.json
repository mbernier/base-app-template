    "@zoralabs/coins-sdk": "^0.4.3",
    "@zoralabs/protocol-sdk": "^0.13.18",
    "clsx": "^2.1.1",
    "iron-session": "^8.0.0",
    "lucide-react": "^0.300.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "siwe": "^2.1.4",
    "viem": "^2.7.0",
    "wagmi": "^2.5.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  }

{
  "name": "base-app-template",
  "version": "1.0.0",
  "private": true,
  "description": "Base Mini App template - Foundation for building Base Mini Apps with SIWE, OnchainKit, and Supabase",
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "next start -p 3100",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "postinstall": "cp node_modules/@coinbase/onchainkit/dist/assets/style.css public/onchainkit.css 2>/dev/null || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^1.1.2",
    "@farcaster/auth-client": "^0.6.0",
    "@farcaster/miniapp-sdk": "^0.2.3",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "@zoralabs/coins-sdk": "^0.4.3",
    "@zoralabs/protocol-sdk": "^0.13.18",
    "clsx": "^2.1.1",
    "ethers": "^6.16.0",
    "iron-session": "^8.0.0",
    "lucide-react": "^0.300.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "siwe": "^2.1.4",
    "viem": "^2.45.1",
    "wagmi": "^2.5.0",
    "zod": "^3.22.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "husky": "^9.1.7",
    "ioredis": "^5.9.2",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
